{
  "version": 2,
  "builds": [
    {
      "src": "index.html",
      "use": "@vercel/static"
    },
    {
      "src": "scripts/**/**",
      "use": "@vercel/static"
    },
    {
      "src": "styles/**/**",
      "use": "@vercel/static"
    },
    {
      "src": "public/**/**",
      "use": "@vercel/static"
    },
    {
      "src": "download.html",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ]
}
